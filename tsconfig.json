{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "ESNext",
    "lib": ["esnext", "DOM"],
    "module": "ES2022",
    "moduleResolution": "node",
    "noEmit": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "strict": true,
    "types": ["vitest/globals", "node"]
  }
}
